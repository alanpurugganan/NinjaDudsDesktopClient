<Window x:Class="NinjaDudsDesktopClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NinjaDudsDesktopClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="650" Width="800">
    <DockPanel>
        <Button Content="Button" DockPanel.Dock="Bottom" Click="Button_Click"/>
        <Image DockPanel.Dock="Left" x:Name="ImageViewer" Width="400"/>
        <Image DockPanel.Dock="Right" x:Name="ImageViewer2" Width="400"/>
        

    </DockPanel>
</Window>
